{
	"name": "@ITSA-Nucleo/itsa-fe-components",
	"private": false,
	"version": "0.0.32",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/itsa-fe-components.es.js",
			"require": "./dist/itsa-fe-components.umd.js"
		},
		"./styles.css": {
			"require": "./dist/styles.css",
			"default": "./dist/styles.css"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"*.css"
	],
	"scripts": {
		"build": "yarn clean && tsc && vite build && yarn build:css",
		"build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
		"clean": "rimraf dist",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"storybook": "concurrently \"yarn storybook:css\" \"storybook dev -p 6006\"",
		"storybook:css": "tailwindcss -w -i ./src/tailwind-entry.css -o ./src/index.css",
		"build-storybook": "concurrently \"yarn build-storybook:css\" \"storybook build\"",
		"build-storybook:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./src/index.css",
		"lint:fix": "eslint . --fix",
		"tscheck": "tsc -p . --noEmit",
		"prettier": "prettier --write \"{src,public}/**/*.{tsx,ts,js,json,md,scss}\"",
		"prettier:check": "prettier --check \"{src,public}/**/*.{tsx,ts,js,json,md,scss}\"",
		"prepare": "husky",
		"test": "vitest --run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"rebase-staging": "git checkout staging && git pull origin staging && git checkout develop && git rebase staging && git pull origin develop",
		"create-package": "yarn build && yarn pack",
		"build-package:staging": "vite build --mode package && yarn build:css",
		"build-package:production": "vite build --mode package --no-sourcemap && yarn build:css",
		"clean:test": "rm -rf .cache node_modules/.cache coverage .vitest .jest tmp",
		"clean:snapshots": "rimraf \"**/__snapshots__/*.snap\""
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{html,json,md,css,scss}": [
			"prettier --write"
		],
		"*.js": [
			"prettier --write"
		],
		"*.{ts,tsx}": [
			"prettier --write"
		]
	},
	"peerDependencies": {
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@fontsource/public-sans": "^5.2.6",
		"@googlemaps/js-api-loader": "^1.16.8",
		"@hookform/resolvers": "^5.2.1",
		"@mdi/js": "^7.4.47",
		"@mdi/react": "^1.6.1",
		"antd": "^5.26.7",
		"axios": "^1.8.4",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.13",
		"query-string": "^9.1.1",
		"react-hook-form": "^7.62.0",
		"react-number-format": "^5.4.3",
		"react-router-dom": "^7.6.0",
		"zod": "^4.1.5",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^1.3.3",
		"@storybook/addon-essentials": "^8.0.9",
		"@storybook/addon-interactions": "^8.0.9",
		"@storybook/addon-links": "^8.0.9",
		"@storybook/addon-onboarding": "^8.0.9",
		"@storybook/blocks": "^8.0.9",
		"@storybook/react": "^8.0.9",
		"@storybook/react-vite": "^8.0.9",
		"@storybook/test": "^8.0.9",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"@types/googlemaps": "^3.43.3",
		"@types/node": "^22.13.5",
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@types/testing-library__jest-dom": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitest/coverage-v8": "^3.0.5",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"eslint-plugin-storybook": "^0.8.0",
		"husky": "^9.1.7",
		"jsdom": "^26.0.0",
		"lint-staged": "^15.4.3",
		"prettier": "^3.5.1",
		"storybook": "^8.0.9",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.2.2",
		"vite": "^5.2.0",
		"vite-plugin-dts": "^3.9.0",
		"vite-plugin-svgr": "^4.3.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.5"
	},
	"resolutions": {
		"@types/react": "^18.3.3"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ITSA-Nucleo/itsa-fe-components.git"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	}
}
